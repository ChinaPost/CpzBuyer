<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com..dao.impl.CpzBuyerMsgMapper">
	<resultMap id="CpzBuyerMsgResultMap" type="com..bean.CpzBuyerMsgBean">
<result column="userId" property="userId" />
<result column="moileNo" property="moileNo" />
<result column="nickName" property="nickName" />
	 <collection property="CpzBuyerCollectShop" ofType="com..bean.CpzBuyerCollectShopBean">
</collection>
	</resultMap>
	<select id="CpzBuyerMsgSelect" resultMap="CpzBuyerMsgResultMap" >
select  t_cpz_buyer_msg.userId, t_cpz_buyer_msg.moileNo, t_cpz_buyer_msg.nickName from t_cpz_buyer_collect_shop inner join t_cpz_buyer_msg on t_cpz_buyer_collect_shop.userId=t_cpz_buyer_msg.userId	</select>
</mapper>

